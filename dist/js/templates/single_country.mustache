<div class="row">
 <div class="col-6">
      <div class="small-box bg-info">
          <div class="inner">
            <h3>{{countryName}}</h3>

            <p>Country</p>
          </div>
          <div class="icon">
            <i class="fas fa-globe-europe"></i>
          </div>
          
      </div>
    
  </div>
  <div class="col-6">
    <div class="small-box bg-success">
          <div class="inner">
            <h3>{{bandValue}}</h3>

            <p>Band</p>
          </div>
          <div class="icon">
            <i class="fas fa-shield-alt"></i>
          </div>

    </div>
   
  </div>
  
  
</div>

{{#questions}}
<div class="row mb-2">
    <div class="col-12">
        <div class="card card-primary card-outline">
            <div class="card-header">
                <h3 class="card-title">
                  
                    <h3>{{question}}</h3>
                </h3>
            </div>
            <div class="card-body" >

                <h4>Score: {{score}}</h4>
                
                <div class="row">
                  <div class="col-5 col-sm-3">
                    <div class="nav flex-column nav-tabs h-100" id="vert-tabs-tab-{{qn}}" role="tablist" aria-orientation="vertical">
                      <a class="nav-link active" id="vert-tabs-home-tab-{{qn}}" data-toggle="pill" href="#vert-tabs-home-{{qn}}" role="tab" aria-controls="vert-tabs-home-{{qn}}" aria-selected="true">Assessor Comments</a>
                      
                      <a class="nav-link" id="vert-tabs-profile-tab-{{qn}}" data-toggle="pill" href="#vert-tabs-profile-{{qn}}" role="tab" aria-controls="vert-tabs-profile-{{qn}}" aria-selected="false">Assessor Sources</a>
                      
                      <a class="nav-link" id="vert-tabs-c1-tab-{{qn}}" data-toggle="pill" href="#vert-tabs-c1-{{qn}}" role="tab" aria-controls="vert-tabs-c1-{{qn}}" aria-selected="false">Reviewers #1</a>
                      
                      <a class="nav-link" id="vert-tabs-c2-tab-{{qn}}" data-toggle="pill" href="#vert-tabs-c2-{{qn}}" role="tab" aria-controls="vert-tabs-c2-{{qn}}" aria-selected="false">Reviewers #2</a>
                      
                      <a class="nav-link" id="vert-tabs-c3-tab-{{qn}}" data-toggle="pill" href="#vert-tabs-c3-{{qn}}" role="tab" aria-controls="vert-tabs-c3-{{qn}}" aria-selected="false">Reviewers #3</a>
                      
                      <a class="nav-link" id="vert-tabs-c4-tab-{{qn}}" data-toggle="pill" href="#vert-tabs-c4-{{qn}}" role="tab" aria-controls="vert-tabs-c4-{{qn}}" aria-selected="false">Reviewers #4</a>
                    </div>
                  </div>
                  <div class="col-7 col-sm-9">
                    <div class="tab-content" id="vert-tabs-tabContent-{{qn}}">
                      <div class="tab-pane text-left fade show active" id="vert-tabs-home-{{qn}}" role="tabpanel" aria-labelledby="vert-tabs-home-tab-{{qn}}">
                         <h5>{{comment}}</h5>
                      </div>
                      <div class="tab-pane fade" id="vert-tabs-profile-{{qn}}" role="tabpanel" aria-labelledby="vert-tabs-profile-tab-{{qn}}">
                         <h5>{{source}}</h5>
                      </div>
                      <div class="tab-pane fade" id="vert-tabs-c1-{{qn}}" role="tabpanel" aria-labelledby="vert-tabs-c1-tab-{{qn}}">
                         <h5>{{c1}}</h5>
                      </div>
                      <div class="tab-pane fade" id="vert-tabs-c2-{{qn}}" role="tabpanel" aria-labelledby="vert-tabs-c2-tab-{{qn}}">
                         <h5>{{c2}}</h5>
                      </div>
                      <div class="tab-pane fade" id="vert-tabs-c3-{{qn}}" role="tabpanel" aria-labelledby="vert-tabs-c3-tab-{{qn}}">
                         <h5>{{c3}}</h5>
                      </div>
                      <div class="tab-pane fade" id="vert-tabs-c4-{{qn}}" role="tabpanel" aria-labelledby="vert-tabs-c4-tab-{{qn}}">
                         <h5>{{c4}}</h5>
                      </div>
                    </div>
                  </div>
                </div>

            </div>
        </div>
    <!-- /.card -->
    </div>
</div>

{{/questions}}